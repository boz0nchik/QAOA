    "from preparator import Preparator \n",
    "import numpy as np\n",
    "\n",
    "prep = Preparator()\n",
    "Ms = []\n",
    "for _ in range (10000):\n",
    "\n",
    "    matrix = np.random.uniform(low = -1, high = 1, size = [10,10])\n",
    "    M = prep.penalty_weight(matrix, 1, 'MOMC')\n",
    "    Ms.append(M)\n",
    "print(np.mean(M))"
   ]
